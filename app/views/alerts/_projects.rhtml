<ul>
<% if alert.apply_to_all_projects?  %>
<li><%= l(:label_all) %></li>
<% elsif alert.projects.any? %>
<% alert.projects.each do |project| %>
<li><%= link_to h(project.name), :controller => 'projects', :action => 'show', :id => project.identifier %></li>
<% end %>
<% else %>
<li><%= l(:label_none) %></li>
<% end %>
</ul>
